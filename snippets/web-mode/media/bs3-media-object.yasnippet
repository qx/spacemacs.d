<div class="media">
	<a class="pull-left" href="${1:#}">
		<img class="media-object" src="${2:#}" alt="${3:Image}">
	</a>
	<div class="media-body">
		<h4 class="media-heading">${4:Media heading}</h4>
		<p>${5:Text goes here...}</p>
	</div>
</div>
